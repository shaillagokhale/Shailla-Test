                                                }
                                                                                                          
                                            }
                                            TotalLOE = TotalLOE.toString();
                                            if(TotalLOE.indexOf(".") == -1) TotalLOE = TotalLOE+".00";
                                            if(TotalLOE == 0) var decTotalLOE = '';
                                            else var decTotalLOE  = parseFloat(TotalLOE).toFixed(2);
