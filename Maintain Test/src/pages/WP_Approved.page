                                        }
                                        function Calculation(){                                            
                                            var size='{!lstwrapServicesType[intIndexST].lstwrapServices[intIndex].lstwrapResource.size}';
                                            var TotalLOE = 0;
                                            var TotalCost =0;
                                            var TotalLFALOE = 0;
