    function changeStartAndEndMonth(cycle){
        //document.getElementById('page:frm:hdnstrSelectedCycle').value = cycle.value;
        
        document.getElementById('page:frm:CycleInput').disabled = true; //TCS Change to make Cycle disable and Pass Value to Controller
       
