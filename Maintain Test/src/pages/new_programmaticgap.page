                if(StartMonthTotal[i].id == 'SlashSpan')
                    StartMonthTotal[i].style.display = 'none';*/
            }
        }
    }
    
    function getMonth(strMonth){
        var strMonthNumber = '';
        if(strMonth == 'January') strMonthNumber = '01';
        if(strMonth == 'February') strMonthNumber = '02'; 
        if(strMonth == 'March') strMonthNumber = '03';
        if(strMonth == 'April') strMonthNumber = '04';
        if(strMonth == 'May') strMonthNumber = '05';
        if(strMonth == 'June') strMonthNumber = '06';
        if(strMonth == 'July') strMonthNumber = '07';
        if(strMonth == 'August') strMonthNumber = '08';
        if(strMonth == 'September') strMonthNumber = '09';
        if(strMonth == 'October') strMonthNumber = '10';
        if(strMonth == 'November') strMonthNumber = '11';
        if(strMonth == 'December') strMonthNumber = '12';
        return strMonthNumber;
    }
    
