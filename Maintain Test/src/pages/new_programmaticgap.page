        function AlertSectionUpdates()
        {
            sforce.connection.sessionId = '{!$Api.Session_ID}';
            var SectionID = '{!Module__c.ID}';
            var result = sforce.connection.query("Select Id,Status__c,LastModifiedBy.Name,LastModifiedDate, concept_note__r.LWBEdited__c from module__c where id ='" + SectionID + "'");
            var records = result.getArray("records");
            var SectionRecord = records[0];
            
            var LastmodifiedBy=SectionRecord.LastModifiedBy.Name;
            
            var result = sforce.connection.getUserInfo();
            var UserName=result.userFullName;
    
            if(UserName!=LastmodifiedBy)
            {
                var LastmodifiedDate = new Date(SectionRecord .LastModifiedDate);
                var Message;
                if(isNaN(LastmodifiedDate ))
                {
                    Message="{!$Label.Section_Last_Modified} "+LastmodifiedBy+" {!$Label.on}  "+SectionRecord .LastModifiedDate+".\n {!$Label.Are_you_sure_save_changes}?";
                }
                else
                {
                    Message="{!$Label.Section_Last_Modified} "+LastmodifiedBy+" {!$Label.on}  "+LastmodifiedDate+".\n {!$Label.Are_you_sure_save_changes}?";
                }
                if(confirm(Message))
                {
                    return true;                
                }
            }
            else
            {
                return true;
            }
            return false;
        }
    
    
    
    
    
    
    
    
