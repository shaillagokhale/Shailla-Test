    function RemoveComma(CostValue){
        
        var ReComma = new RegExp("\\,","g");
        var ReQuate = new RegExp("\\'","g");
        CostValue = CostValue.replace(ReComma,"");
        CostValue = CostValue.replace(ReQuate,"");
        CostValue = CostValue.replace(",.",".");
        CostValue = CostValue.replace("'.",".");
        return CostValue;
    }
    
