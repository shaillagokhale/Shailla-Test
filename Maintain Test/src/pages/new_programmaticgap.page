    function nthLastIndexOf(str, c, n) {
        if (str == null || n < 1)
            return -1;
        var pos = str.length;
        while (n-- > 0 && pos != -1)
            pos = str.lastIndexOf(c, pos - 1);
        return pos;
    }
        
