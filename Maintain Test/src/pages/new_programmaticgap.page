                for(var i=0;i<StartMonthTotal.length;i++){
                    
                    if(StartMonthTotal[i].id == 'txtSM')
                        StartMonthTotal[i].innerHTML = getMonth(StartMonth);
                    if(StartMonthTotal[i].id == 'txtEM')
                        StartMonthTotal[i].innerHTML = getMonth(EndMonth);
                    if(StartMonthTotal[i].id == 'SlashSpan')
                        StartMonthTotal[i].style.display = '';
                    if(cycle.value == 'January - December'){
