    function calcValue(txtTvalue,DataType,txtANume,txtADnomi,decimal){
        
        var Nume= GeOnlyNumber(document.getElementById(txtANume).value);
        var Dnomi= GeOnlyNumber(document.getElementById(txtADnomi).value);
        var objValue = document.getElementById(txtTvalue);
        var objopValue = document.getElementById(txtTvalue);
        var decimal1 = document.getElementById(decimal).value;
        if(DataType=="Ratio"){
            if(Dnomi==0){
                 objValue.value="";
                 objopValue.innerHTML = "";
            }else{
                var Value=Nume/(Dnomi/100000);
                objValue.value=Value.toFixed(decimal1);
                objopValue.innerHTML=Value.toFixed(decimal1);
            }
        }else if(DataType=="Percent"||DataType=="Number and Percent"){
            if(Dnomi==0){
                 objValue.value="";
                 objopValue.innerHTML="";
            }else{
                var Value=(parseFloat((parseFloat(Nume)/parseFloat(Dnomi)))*100);
                objValue.value=parseFloat(Value).toFixed(decimal1);
                objopValue.innerHTML=Value.toFixed(decimal1);
            }
        }
    }
    
