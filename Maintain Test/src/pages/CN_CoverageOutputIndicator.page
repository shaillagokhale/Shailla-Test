                <script type="text/javascript">
                  function validateStandNumAndPer(){
                    var i;
                var SelectedPRSize=document.getElementById('PRsize').value;
       
                for(i=0;i<SelectedPRSize;i++)
                {
                var datatypestand=document.getElementById('page:frm:PR:'+i+':DataTypeStand').value;
                var decimalplace1= document.getElementById('page:frm:PR:'+i+':ipdecimalplacestand').value;
                if(decimalplace1== '')
                    document.getElementById(decimal).value = 0;
                document.getElementById('tableCusStdBtn').style.display = '';
                if(datatypestand == 'Number and Percent'){
                if(!isNaN(document.getElementById('page:frm:PR:'+i+':txtBNume').value) && document.getElementById('page:frm:PR:'+i+':txtBNume').value!= '')
                    document.getElementById('page:frm:txtBNume').value = parseFloat(document.getElementById('page:frm:PR:'+i+':txtBNume').value).toFixed(decimalplace1);
                if(!isNaN(document.getElementById('page:frm:PR:'+i+':txtBDnomi').value) && document.getElementById('page:frm:PR:'+i+':txtBDnomi').value!= '')
                    document.getElementById('page:frm:PR:'+i+':txtBDnomi').value = parseFloat(document.getElementById('page:frm:PR:'+i+':txtBDnomi').value).toFixed(decimalplace1);
                if(!isNaN(document.getElementById('page:frm:PR:'+i+':EditStdNum1').value) && document.getElementById('page:frm:PR:'+i+':EditStdNum1').value!= '')
                    document.getElementById('page:frm:PR:'+i+':EditStdNum1').value = parseFloat(document.getElementById('page:frm:PR:'+i+':EditStdNum1').value).toFixed(decimalplace1);
